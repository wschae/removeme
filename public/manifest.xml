<?xml version="1.0" encoding="UTF-8"?>
    <!--Created:cb85b80c-f585-40ff-8bfc-12ff4d0e34a9-->
    <OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:type="MailApp">
        <Id>971E76EF-D73E-567F-ADAE-9876B39052CF</Id>
        <Version>1.0.0.0</Version>
        <ProviderName>Microsoft</ProviderName>
        <DefaultLocale>en-US</DefaultLocale>
        <DisplayName DefaultValue="Remove Me"/>
        <Description DefaultValue="Stop Reply All Threads When Everyone Says 'Remove Me From This DL'">
        </Description>
        <IconUrl DefaultValue="http://removeme.azurewebsites.net/assets/icon-32.png"/>
        <Hosts>
            <Host Name="Mailbox" />
        </Hosts>
        <Requirements>
            <Sets>
                <Set Name="MailBox" MinVersion="1.1" />
            </Sets>
        </Requirements>
        <FormSettings>
            <Form xsi:type="ItemRead">
                <DesktopSettings>
                    <SourceLocation DefaultValue="http://removeme.azurewebsites.net/removeme.html"/>
                    <RequestedHeight>108</RequestedHeight>
                </DesktopSettings>
                <TabletSettings>
                    <SourceLocation DefaultValue="http://removeme.azurewebsites.net/removeme.html"/>
                    <RequestedHeight>108</RequestedHeight>
                </TabletSettings>
            </Form>
        </FormSettings>
        <Permissions>ReadItem</Permissions>
        <Rule xsi:type="RuleCollection" Mode="Or">
            <Rule xsi:type="ItemIs" ItemType="Message" FormType="Read" />
            <Rule xsi:type="ItemIs" ItemType="Appointment" FormType="Read" />
            <Rule xsi:type="ItemHasKnownEntity" EntityType="EmailAddress" />
        </Rule>
        <DisableEntityHighlighting>false</DisableEntityHighlighting>
</OfficeApp>